{
  "name": "SuperFeed",
  "version": "0.0.0",
  "author": "Ryan Delaney <rrdelaney@outlook.com>",
  "repository": "https://github.com/SuperFeed/Front",
  "license": "MIT",
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "test/*"
    ],
    "globals": [
      "fetch",
      "FormData"
    ]
  },
  "apidoc": {
    "title": "SFAPI docs",
    "sampleUrl": "https://reoz4sq35d.execute-api.us-east-1.amazonaws.com/prod"
  },
  "scripts": {
    "test": "standard | snazzy",
    "docs": "apidoc -i functions",
    "dev-client": "webpack-dev-server --history-api-fallback --hot --inline --progress --colors --port 3000",
    "build-client": "cross-env NODE_ENV=production webpack",
    "predev-server": "cross-env BABEL_ENV=server babel src -d _server",
    "dev-server": "cross-env BABEL_ENV=server parallelshell \"babel --watch src -d _server\" \"nodemon --watch _server _server/server.js\"",
    "build-server": "BABEL_ENV=server babel src -d target",
    "predev": "rimraf _client _server && mkdirp _static",
    "dev": "parallelshell \"npm run dev-client\" \"npm run dev-server\"",
    "prebuild": "rimraf target && mkdirp target",
    "build": "npm run build-server && npm run build-client",
    "postbuild": "cp package.json target && cp -r resources/. target",
    "prod": "../node_modules/.bin/cross-env STATIC=local NODE_ENV=production PORT=3000 node server.js",
    "create-db": "node -e \"require('./db').createDatabase()\"",
    "drop-db": "node -e \"require('./db').dropDatabase()\"",
    "seed-db": "node -e \"require('./db').seedDatabase()\"",
    "setup-db": "npm run drop-db && npm run create-db && npm run seed-db"
  },
  "devDependencies": {
    "apidoc": "^0.15.1",
    "babel-cli": "^6.6.5",
    "babel-core": "^6.5.2",
    "babel-eslint": "^6.0.2",
    "babel-loader": "^6.2.3",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.5.2",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.7.2",
    "cross-env": "^1.0.7",
    "css-loader": "^0.23.1",
    "eslint": "^2.6.0",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.9.0",
    "html-webpack-template": "^4.0.0",
    "json-loader": "^0.5.4",
    "mkdirp": "^0.5.1",
    "nodemon": "^1.9.0",
    "parallelshell": "^2.0.0",
    "rimraf": "^2.5.2",
    "snazzy": "^3.0.0",
    "standard": "^6.0.8",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.13",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "babel-polyfill": "^6.5.0",
    "babel-runtime": "^6.6.1",
    "instagram-node": "^0.5.8",
    "isomorphic-fetch": "^2.2.1",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-dropzone": "^3.4.0",
    "react-facebook-login": "SuperFeed/react-facebook-login",
    "react-redux": "^4.4.0",
    "react-router": "^2.0.1",
    "react-router-redux": "^4.0.1",
    "redux": "^3.3.1",
    "redux-actions": "^0.9.1",
    "redux-devtools": "^3.2.0",
    "redux-devtools-dispatch": "^2.1.0",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.0.9",
    "redux-devtools-multiple-monitors": "^1.0.1",
    "redux-promise": "^0.5.1",
    "redux-reaction": "^0.1.0",
    "redux-slider-monitor": "^1.0.3",
    "reqq": "^0.2.1",
    "rethinkdb": "^2.3.0",
    "soular": "^0.3.7",
    "twitter": "^1.2.5"
  }
}
